{
  "name": "@letscollab/lets-community",
  "version": "0.0.1",
  "description": "letscollab BaaS community edition",
  "author": "Han",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "bundle": "npm run prebuild && ncc build ./src/main.ts -o dist -m",
    "bundle:dev": "npm run prebuild && ncc build ./src/main.ts -o dist -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.300.0",
    "@fastify/http-proxy": "^9.0.0",
    "@fastify/swagger": "7.5.0",
    "@liaoliaots/nestjs-redis": "8.0.0",
    "@nestjs-modules/mailer": "1.7.1",
    "@nestjs/common": "9.3.11",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.11",
    "@nestjs/platform-fastify": "9.3.11",
    "@nestjs/swagger": "6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "*",
    "fastify": "4.4.0",
    "ioredis": "5.0.4",
    "mysql2": "2.3.3",
    "nest-winston": "1.6.2",
    "nestjs-i18n": "9.2.0",
    "otplib": "12.0.1",
    "p-retry": "5.1.1",
    "passport-github2": "0.1.12",
    "passport-local": "1.0.0",
    "passport-paseto": "0.0.0-alpha.3",
    "qrcode": "1.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "sharp": "^0.32.0",
    "typeorm": "0.3.7",
    "typeorm-adapter": "1.5.1",
    "winston": "3.7.2",
    "winston-loki": "6.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.1",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.11",
    "@types/bcrypt": "5",
    "@types/jest": "29.4.1",
    "@types/node": "18.0.0",
    "@types/passport": "0",
    "@types/passport-github2": "1.2.5",
    "@types/passport-local": "1",
    "@types/qrcode": "1",
    "@types/rimraf": "3",
    "@types/supertest": "2.0.11",
    "@vercel/ncc": "0.34.0",
    "cross-env": "7.0.3",
    "jest": "27.2.5",
    "nodemon": "2.0.15",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
