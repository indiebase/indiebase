{
  "name": "@letscollab/user",
  "version": "0.0.1",
  "description": "User service",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "./src/index.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "docker:prod": "npm run bundle && docker build . -t letscollab/user  --build-arg ENV=production",
    "docker:local": "npm run bundle && docker build --build-arg RUNTIME_ENV=local --no-cache -t letscollab/user-local .",
    "start": "cross-env NODE_ENV=development nodemon -e ts,js --exec ts-node --swc ./src/main.ts",
    "preview:dev": "npm run bundle:dev && fse copy --quiet ../../node_modules/swagger-ui-dist ./dist/swagger-ui-dist && cross-env NODE_ENV=development node ./dist/index.js",
    "bundle": "npm run prebuild && ncc build ./src/main.ts -o dist -m",
    "bundle:dev": "npm run prebuild && ncc build ./src/main.ts -o dist -s",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@fastify/cookie": "8.1.0",
    "@fastify/helmet": "^10.0.0",
    "@letscollab-nest/nacos": "workspace:^",
    "@letscollab-nest/trait": "workspace:^",
    "@letscollab/auth": "workspace:^",
    "@letscollab/collab": "workspace:^",
    "@letscollab/helper": "workspace:^",
    "@liaoliaots/nestjs-redis": "^8.0.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/microservices": "^9.0.11",
    "@nestjs/platform-fastify": "^9.0.11",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "amqp-connection-manager": "^4.1.6",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^9.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "winston": "^3.7.2",
    "winston-loki": "^6.0.5"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@swc/core": "^1.3.14",
    "@swc/helpers": "^0.4.12",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^5",
    "@types/jest": "27.0.2",
    "@types/mysql": "^2",
    "@types/node": "^16.0.0",
    "@types/rimraf": "^3",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.2",
    "@vercel/ncc": "^0.34.0",
    "cross-env": "^7.0.3",
    "jest": "^27.2.5",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
