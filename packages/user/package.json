{
  "name": "@letscollab/user",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "./src/index.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:docker": "npm run ncc && docker build . -t letscollab/user  --build-arg ENV=production",
    "build": "nest build",
    "ncc": "npm run prebuild && ncc build ./src/main.ts -o dist -m",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && cross-env NODE_ENV=development nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/main.ts",
    "start:debug": "npm run prebuild && cross-env NODE_ENV=development nest start -d -w",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@letscollab/auth": "workspace:^",
    "@letscollab/collab": "workspace:^",
    "@letscollab/helper": "workspace:^",
    "@letscollab/nest-nacos": "workspace:^",
    "@liaoliaots/nestjs-redis": "^8.0.0",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-fastify": "^8.4.4",
    "@nestjs/typeorm": "^8.0.3",
    "amqp-connection-manager": "^4.1.1",
    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-cookie": "5.6.1",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^9.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.42",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "winston-loki": "^6.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/amqplib": "^0",
    "@types/bcrypt": "^5",
    "@types/jest": "27.0.2",
    "@types/mysql": "^2",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/rimraf": "^3",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.33.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
