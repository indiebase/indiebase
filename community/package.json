{
  "name": "@indiebase/community",
  "version": "0.0.1",
  "description": "indiebase BaaS community edition",
  "author": "Han",
  "private": true,
  "type": "commonjs",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "docker:prod": "npm run bundle && docker build --build-arg RUNTIME_ENV=production -t indiebase/community.",
    "docker:local": "npm run bundle && docker build --build-arg RUNTIME_ENV=local --no-cache -t indiebase/community-local .",
    "build": "rimraf build && tsc -b --clean && tsc -b -p ./tsconfig.build.json",
    "start:tsx": "cross-env NODE_ENV=development DEBUG=\"ioredis:*\" tsx watch ./src/main.ts",
    "start": "cross-env NODE_ENV=development DEBUG=\"ioredis:*\" webpack --config webpack.config.js --watch",
    "preview:dev": "cross-env NODE_ENV=development node ./build/index.js",
    "bundle": "rimraf dist && ncc build ./src/main.ts -o dist -m",
    "bundle:dev": "rimraf dist && ncc build ./src/main.ts -o dist -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "paseto:key": "node ./scripts/paseto.mjs"
  },
  "dependencies": {
    "@deskbtm/gadgets": "workspace:^",
    "@fastify/cookie": "8.3.0",
    "@fastify/helmet": "11.0.0",
    "@fastify/multipart": "7.7.3",
    "@fastify/passport": "2.3.0",
    "@fastify/session": "9.0.0",
    "@fastify/swagger": "8.9.0",
    "@indiebase/nest-casl": "workspace:^",
    "@indiebase/nest-fastify-passport": "workspace:^",
    "@indiebase/nest-knex": "workspace:^",
    "@indiebase/nest-kysely": "workspace:^",
    "@indiebase/nest-octokit": "workspace:^",
    "@indiebase/nest-stoplight-elements": "workspace:^",
    "@indiebase/sdk": "workspace:^",
    "@indiebase/server-shared": "workspace:^",
    "@indiebase/trait": "workspace:^",
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@nestjs-modules/mailer": "1.7.1",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.1.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-fastify": "10.1.3",
    "@nestjs/swagger": "7.1.8",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dayjs": "^1.11.9",
    "fastify": "4.21.0",
    "ioredis": "5.0.4",
    "knex": "^2.5.1",
    "kysely": "0.26.3",
    "nest-winston": "1.9.4",
    "nestjs-i18n": "10.2.6",
    "nestjs-paseto": "workspace:^",
    "objection": "^3.1.2",
    "otplib": "12.0.1",
    "passport-github2": "0.1.12",
    "passport-google-oauth20": "2.0.0",
    "passport-local": "1.0.0",
    "passport-paseto": "0.0.1-alpha.2",
    "pg": "8.11.3",
    "qrcode": "1.5.3",
