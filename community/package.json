{
  "name": "@indiebase/community",
  "version": "0.0.1",
  "description": "indiebase BaaS community edition",
  "author": "Han",
  "private": true,
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "docker:prod": "npm run bundle && docker build --build-arg RUNTIME_ENV=production -t indiebase/community.",
    "docker:local": "npm run bundle && docker build --build-arg RUNTIME_ENV=local --no-cache -t indiebase/community-local .",
    "build": "rimraf build && tsc -b --clean && tsc -b -p ./tsconfig.build.json",
    "start": "tsc -b && cross-env NODE_ENV=development DEBUG=\"ioredis:*\" webpack --config webpack.config.js --watch",
    "preview:dev": "cross-env NODE_ENV=development node ./build/index.js",
    "bundle": "rimraf dist && ncc build ./src/main.ts -o dist -m",
    "bundle:dev": "rimraf dist && ncc build ./src/main.ts -o dist -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/helmet": "11.0.0",
    "@fastify/multipart": "7.7.0",
    "@fastify/passport": "2.3.0",
    "@fastify/session": "9.0.0",
    "@fastify/swagger": "8.8.0",
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@nestjs-modules/mailer": "1.7.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.0.5",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.5",
    "@nestjs/platform-fastify": "10.0.5",
    "@nestjs/swagger": "7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "^1.11.9",
    "fastify": "4.4.0",
    "ioredis": "5.0.4",
    "knex": "^2.5.1",
    "nest-winston": "1.6.2",
    "nestjs-i18n": "10.2.6",
    "otplib": "12.0.1",
    "p-retry": "5.1.2",
    "passport-github2": "0.1.12",
    "passport-google-oauth20": "2.0.0",
    "passport-local": "1.0.0",
    "passport-paseto": "0.0.0-alpha.3",
    "pg": "8.11.1",
    "qrcode": "1.5.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.9",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@types/bcrypt": "5",
    "@types/node": "20.4.5",
    "@types/passport": "1.0.12",
    "@types/passport-github2": "1.2.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "1.0.35",
    "@types/pg": "^8",
    "@types/qrcode": "1.5.0",
    "@types/rimraf": "4.0.5",
    "@types/supertest": "2.0.11",
    "@vercel/ncc": "0.34.0",
    "cross-env": "7.0.3",
    "esbuild-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "run-script-webpack-plugin": "0.2.0",
    "source-map-support": "0.5.20",
    "supertest": "6.3.3",
    "swc-loader": "0.2.3",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
