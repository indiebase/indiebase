{
  "name": "@indiebase/community",
  "version": "0.0.1",
  "description": "Indiebase community edition",
  "author": "Han",
  "private": true,
  "license": "Apache-2.0",
  "type": "commonjs",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "docker:prod": "npm run bundle && docker build --build-arg RUNTIME_ENV=production -t indiebase/community.",
    "docker:local": "npm run bundle && docker build --build-arg RUNTIME_ENV=local --no-cache -t indiebase/community-local .",
    "build": "rimraf build && tsc -b --clean && tsc -b -p ./tsconfig.build.json",
    "start": "cross-env NODE_ENV=development DEBUG=\"ioredis:*\" webpack --config webpack.config.js --watch",
    "preview:dev": "cross-env NODE_ENV=development node ./build/index.js",
    "bundle": "rimraf dist && ncc build ./src/main.ts -o dist -m",
    "bundle:dev": "rimraf dist && ncc build ./src/main.ts -o dist -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "paseto:key": "node ./scripts/paseto.mjs"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/multipart": "8.1.0",
    "@fastify/passport": "2.4.0",
    "@fastify/session": "10.7.0",
    "@fastify/swagger": "8.14.0",
    "@indiebase/nest-ac": "workspace:^",
    "@indiebase/nest-async-context": "workspace:^",
    "@indiebase/nest-fastify-file": "workspace:^",
    "@indiebase/nest-fastify-passport": "workspace:^",
    "@indiebase/nest-knex": "workspace:^",
    "@indiebase/nest-kysely": "workspace:^",
    "@indiebase/nest-octokit": "workspace:^",
    "@indiebase/nest-stoplight-elements": "workspace:^",
    "@indiebase/nestjs-redis": "10.0.0",
    "@indiebase/sdk": "workspace:^",
    "@indiebase/server-shared": "workspace:^",
    "@indiebase/trait": "workspace:^",
    "@nestjs-modules/mailer": "1.11.2",
    "@nestjs/axios": "3.0.2",
    "@nestjs/common": "10.3.5",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.5",
    "@nestjs/platform-fastify": "10.3.5",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/terminus": "10.2.3",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "1.11.10",
    "fastify": "4.26.2",
    "hyperid": "3.2.0",
    "ioredis": "5.3.2",
    "knex": "3.1.0",
    "ms": "2.1.3",
    "nest-winston": "1.9.4",
    "nestjs-i18n": "10.4.5",
    "nestjs-paseto": "workspace:^",
    "otplib": "12.0.1",
    "passport-github2": "0.1.12",
    "passport-google-oauth20": "2.0.0",
    "passport-local": "1.0.0",
    "passport-paseto": "0.1.0-alpha.2",
    "pg": "8.11.3",
    "qrcode": "1.5.3",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "winston": "3.12.0",
    "winston-openobserve": "workspace:^"
  },
  "devDependencies": {
    "@indiebase/dev": "workspace:^",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.5",
    "@types/bcrypt": "5.0.2",
    "@types/node": "20.8.10",
    "@types/passport": "1.0.16",
    "@types/passport-github2": "1.2.9",
    "@types/passport-google-oauth20": "2.0.14",
    "@types/passport-local": "1.0.38",
    "@types/pg": "8.11.2",
    "@types/qrcode": "1.5.5",
    "@types/supertest": "6.0.2",
    "@vercel/ncc": "0.38.1",
    "@vitest/coverage-v8": "1.4.0",
    "cross-env": "7.0.3",
    "jest": "29.6.1",
    "nodemon": "3.0.1",
    "prettier": "3.2.5",
    "shx": "0.3.4",
    "supertest": "6.3.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2",
    "unplugin-swc": "1.4.4",
    "vitest": "1.4.0",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4"
  }
}
