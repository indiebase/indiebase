{
  "name": "indiebase",
  "version": "0.0.2-alpha.0",
  "description": "",
  "author": "Han",
  "private": true,
  "license": "AGPL-3.0",
  "engines": {
    "yarn": ">2.0.0",
    "node": ">=20.0.0",
    "npm": "please-use-yarn",
    "pnpm": "please-use-yarn"
  },
  "scripts": {
    "lint": "yarn workspaces foreach run lint",
    "stack:prod": "docker stack deploy -c ./docker-compose.yaml -c ./docker-compose.production.yaml indiebase",
    "stack:dev": "docker stack deploy -c ./docker-compose.yaml indiebase",
    "release": "standard-version",
    "prepare": "husky install",
    "syncpack": "syncpack",
    "mismatches": "syncpack list-mismatches"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitest/ui": "^0.33.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.1",
    "prettier": "3.0.0",
    "standard-version": "9.3.1",
    "syncpack": "^11.2.1",
    "typescript": "5.2.2"
  },
  "workspaces": {
    "packages": [
      "first_party/*",
      "apps/*",
      "apps/first_party/*",
      "apps/packages/*",
      "community",
      "pro"
    ],
    "nohoist": [
      "**/ky/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.6.3",
  "resolutions": {
    "webpack@5.73.0": "patch:webpack@npm%3A5.79.0#./.yarn/patches/webpack-npm-5.79.0-596061f3bf.patch",
    "webpack@5.79.0": "patch:webpack@npm%3A5.79.0#./.yarn/patches/webpack-npm-5.79.0-596061f3bf.patch",
    "webpack@^5": "patch:webpack@npm%3A5.79.0#./.yarn/patches/webpack-npm-5.79.0-596061f3bf.patch",
    "webpack@5.64.4": "patch:webpack@npm%3A5.79.0#./.yarn/patches/webpack-npm-5.79.0-596061f3bf.patch"
  },
  "dependencies": {
    "hyperid": "^3.1.1",
    "ms": "^2.1.3"
  }
}
