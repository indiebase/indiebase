{
  "name": "letscollab",
  "version": "0.0.2-alpha.0",
  "description": "",
  "author": "Nawbc",
  "private": true,
  "license": "AGPL-3.0",
  "engines": {
    "node": ">= 16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">2.0.0",
    "pnpm": "please-use-yarn"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "release": "standard-version",
    "migration:generate": "cross-env NODE_ENV=production typeorm migration:generate -n SchemaMigration",
    "migration:create": "cross-env NODE_ENV=production typeorm migration:create -n SchemaMigration",
    "migration:run": "cross-env NODE_ENV=production ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "cross-env NODE_ENV=production ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "aws-sdk": "^2.999.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "mysql": "^2.18.1",
    "mysqldump": "^3.2.0",
    "nanoid": "^3.1.25",
    "nestjs-redis": "^1.3.3",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "semver": "^7.3.5",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.1",
    "@types/node-schedule": "^1.3.2",
    "@types/passport-local": "^1.0.34",
    "@types/semver": "^7.3.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "@vercel/ncc": "^0.33.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "27.2.0",
    "prettier": "^2.4.0",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "workspaces": [
    "packages/*",
    "first_party/*",
    "website/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
