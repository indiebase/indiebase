{
  "name": "@letscollab/nest-octokit",
  "version": "0.0.1",
  "description": "Github api nestjs module for letscollab",
  "author": "Han",
  "private": true,
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.js"
  },
  "homepage": "https://github.com/deskbtm-letscollab/mbdpay.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/deskbtm-letscollab/mbdpay.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && nest build --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "octokit": "1.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.1",
    "@nestjs/common": "9.3.11",
    "@nestjs/core": "9.3.11",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.11",
    "@types/jest": "29.4.1",
    "@types/node": "18.0.0",
    "@types/supertest": "2.0.11",
    "jest": "27.2.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
