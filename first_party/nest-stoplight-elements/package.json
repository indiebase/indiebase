{
  "name": "@indiebase/nest-stoplight-elements",
  "version": "0.0.1-alpha.10",
  "description": "Stoplight elements for Nestjs",
  "author": "Han <han@deskbtm.com> ",
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "repository": {
    "url": "https://github.com/indiebase/indiebase",
    "directory": "first_party/nest-stoplight-elements",
    "type": "git"
  },
  "files": [
    "dist",
    "src",
    "views"
  ],
  "scripts": {
    "build": "nest build",
    "prepack": "shx rm -r dist && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:bundle": "ncc build ./example/main.ts -o dist -m",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/static": "7.0.1",
    "@nestjs/common": "10.3.5",
    "@nestjs/core": "10.3.5",
    "@nestjs/platform-fastify": "10.3.5",
    "@nestjs/swagger": "7.3.0",
    "handlebars": "4.7.8",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.5",
    "@types/node": "20.11.30",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.6.1",
    "prettier": "3.2.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  },
  "keywords": [
    "swagger",
    "stoplight",
    "swagger-ui",
    "openapi",
    "api",
    "nestjs",
    "stoplight-elements",
    "redoc"
  ]
}