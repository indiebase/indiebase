{
  "name": "@indiebase/nest-stoplight-elements",
  "version": "0.0.1-alpha.9",
  "description": "Stoplight elements for Nestjs",
  "author": "Han",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/indiebase/indiebase"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "views"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prepublish": "rimraf tsconfig.build.tsbuildinfo && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:bundle": "ncc build ./example/main.ts -o dist -m",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/static": "6.10.2",
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-fastify": "10.1.3",
    "@nestjs/swagger": "7.1.8",
    "handlebars": "4.7.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.12",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/node": "20.4.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [
    "swagger",
    "stoplight",
    "swagger-ui",
    "openapi",
    "api",
    "nestjs",
    "stoplight-elements",
    "redoc"
  ]
}
