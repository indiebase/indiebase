{
  "name": "@indiebase/nest-fastify-passport",
  "version": "8.2.1",
  "description": "Nest - modern, fast, powerful node.js web framework (@passport)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "prebuild": "shx rm dist",
    "build": "shx rm dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "test": "jest"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "passport": "^0.4.0 || ^0.5.0 || ^0.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.0",
    "@commitlint/config-angular": "19.1.0",
    "@nestjs/common": "10.3.3",
    "@nestjs/core": "10.3.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.3",
    "@nestjs/platform-fastify": "10.3.3",
    "@nestjs/testing": "10.3.3",
    "@types/node": "20.8.10",
    "@types/passport": "1.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "lint-staged": "15.2.2",
    "pactum": "3.1.9",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "prettier": "3.0.0",
    "reflect-metadata": "0.1.13",
    "release-it": "15.0.0",
    "rxjs": "7.8.1",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/passport"
  },
  "dependencies": {
    "@fastify/passport": "2.4.0"
  }
}
