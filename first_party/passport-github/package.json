{
  "name": "@letscollab/passport-github",
  "version": "0.0.1",
  "description": "Passport github nodule for letscollab",
  "author": "Han",
  "private": true,
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.js"
  },
  "homepage": "https://github.com/deskbtm-letscollab/mbdpay.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/deskbtm-letscollab/mbdpay.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && nest build --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "p-retry": "^5.1.1",
    "passport-oauth2": "^1.6.1",
    "retry": "^0.13.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1",
    "@types/node": "^16.0.0",
    "@types/passport-oauth2": "^1",
    "@types/retry": "^0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
