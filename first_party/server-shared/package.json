{
  "name": "@indiebase/server-shared",
  "version": "0.0.1",
  "description": "Common utils for indiebase backend",
  "author": "Han",
  "private": true,
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && nest build --watch",
    "start:debug": "npm run prebuild && cross-env NODE_ENV=development nest start -d -w",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "peerDependencies": {
    "@fastify/session": "9.0.0",
    "@indiebase/nest-knex": "workspace:^",
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-fastify": "10.1.3",
    "@nestjs/swagger": "7.1.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "ioredis": "5.0.4",
    "node-forge": "1.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.12",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/connect-redis": "0.0.20",
    "@types/fs-extra": "9.0.13",
    "@types/node": "20.4.5",
    "@types/node-forge": "1.0.2",
    "@types/supertest": "2.0.11",
    "@types/validator": "13.7.10",
    "cross-env": "7.0.3",
    "jest": "29.6.1",
    "source-map-support": "0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
