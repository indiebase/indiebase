{
  "name": "@letscollab/server-shared",
  "version": "0.0.1",
  "description": "Common utils for letscollab backend",
  "author": "Han",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && nest build --watch",
    "start:debug": "npm run prebuild && cross-env NODE_ENV=development nest start -d -w",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -r ./dist"
  },
  "dependencies": {
    "@fastify/session": "9.0.0",
    "@liaoliaots/nestjs-redis": "8.0.0",
    "@nestjs/common": "9.3.11",
    "@nestjs/core": "9.3.11",
    "@nestjs/platform-fastify": "9.3.11",
    "@nestjs/swagger": "6.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "connect-redis": "6.1.3",
    "ioredis": "5.0.4",
    "node-forge": "1.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.1",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.11",
    "@types/connect-redis": "0.0.20",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.4.1",
    "@types/node": "18.0.0",
    "@types/node-forge": "1.0.2",
    "@types/supertest": "2.0.11",
    "@types/validator": "13.7.10",
    "cross-env": "7.0.3",
    "jest": "27.2.5",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
