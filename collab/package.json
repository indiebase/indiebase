{
  "name": "@letscollab/collab",
  "version": "0.0.1",
  "description": "letscollab community edition includes collab, lets-community",
  "author": "Han",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "docker:prod": "npm run bundle && docker build --build-arg RUNTIME_ENV=production -t letscollab/community.",
    "docker:local": "npm run bundle && docker build --build-arg RUNTIME_ENV=local --no-cache -t letscollab/community-local .",
    "start": "cross-env NODE_ENV=development DEBUG=\"ioredis:*\" nodemon -e ts,js --exec ts-node ./src/main.ts",
    "start:wp": "npm run prebuild && cross-env NODE_ENV=development DEBUG=\"ioredis:*\" webpack --config webpack.config.js --watch",
    "preview:dev": "cross-env NODE_ENV=development node ./build/index.js",
    "bundle": "rimraf dist && ncc build ./src/main.ts -o build -m",
    "bundle:dev": "rimraf dist && ncc build ./src/main.ts -o build -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/cookie": "8.1.0",
    "@fastify/helmet": "10.0.0",
    "@fastify/multipart": "7.3.0",
    "@fastify/passport": "2.2.0",
    "@fastify/session": "9.0.0",
    "@fastify/swagger": "7.5.0",
    "@juicyllama/nestjs-redoc": "^2.3.10",
    "@liaoliaots/nestjs-redis": "8.0.0",
    "@nestjs/common": "9.3.11",
    "@nestjs/core": "9.3.11",
    "@nestjs/platform-fastify": "9.3.11",
    "@nestjs/swagger": "6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "*",
    "fastify": "4.4.0",
    "ioredis": "5.0.4",
    "mysql2": "2.3.3",
    "nest-winston": "1.6.2",
    "nestjs-i18n": "9.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.1",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.11",
    "@types/bcrypt": "5",
    "@types/jest": "27.0.2",
    "@types/node": "18.0.0",
    "@types/passport": "0",
    "@types/passport-github2": "1.2.5",
    "@types/passport-local": "1",
    "@types/qrcode": "1",
    "@types/rimraf": "3",
    "@types/supertest": "2.0.11",
    "@types/webpack-node-externals": "^3",
    "@vercel/ncc": "0.34.0",
    "cross-env": "7.0.3",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "jest": "27.2.5",
    "nodemon": "2.0.15",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.8.4",
    "webpack": "5.79.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "@nestjs/config": "2.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
